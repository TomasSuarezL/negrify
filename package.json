    "isbot": "^5.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.164.1",
    "tiny-invariant": "^1.3.3",
    "zod": "^3.23.5"
  },
